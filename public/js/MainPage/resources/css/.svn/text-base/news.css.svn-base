.ournewsitem{
	font-size:12px;
	text-align: justify;
	
}
.ournewsitem .title{
	font-weight:bold;
	margin-bottom:5px;
	font-size:14px;
}
.ournewsitem .titleOfOne{
	font-weight:bold;
	margin: 20px 20px 5px;
	font-size:20px;
	
}
.ournewsitem .image{
	float:left;
	margin: 0 10px 10px 0;	
}

.ournewsitem .anounce{
	width:100% !important;
	overflow: visible !important;
	white-space: normal !important;
}
.ournewsitem .content{
	float: right;
	overflow: visible !important;
	white-space: normal !important;
	margin: 0 20px 10px;
}
.ournewsitem .dateofnew{
	font-size: 10px !important;
	color:  #A39292 !important;
	margin-bottom: 10px;
}
.ournewsitem .dateofonenew{
	font-size: 10px !important;
	color:  #A39292 !important;
	margin-bottom: 10px;
	margin-left: 20px;
}
.readmore {
	float: right;
	margin: 0px;
	
}
.authorofnew{
	font-size: 10px !important;
	color:  #A39292 !important;
	text-decoration: none;
	
}
.authorofonenew{
	font-size: 10px !important;
	color:  #A39292 !important;
	text-decoration: none;
	margin-left: 20px;
}
.iconclass{
	width: 2px;
	height: 2px;
}
.dateofcomments{
	font-size: 10px !important;
	color:  #A39292 !important;
	margin-bottom: 0px;
}



